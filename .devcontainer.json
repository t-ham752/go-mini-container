{
  "name": "go",
  "dockerComposeFile": ["./docker-compose.yml"],
  "service": "ubuntu",
  "remoteUser": "root",
  "workspaceFolder": "/var/lib/go-mini-container",
  "customizations": {
    "vscode": {
      "extensions": ["golang.go"],
      "settings": {
        "editor.formatOnSave": true,
        "[go]": {
          "editor.defaultFormatter": "golang.go"
        }
      }
    }
  }
}
